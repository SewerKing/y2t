{
	"name": "y2t-vscode-tool",
	"displayName": "y2t",
	"description": "yapi generation tool",
	"version": "0.0.1",
	"license": "MIT",
	"author": "Wynne, Ben",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
